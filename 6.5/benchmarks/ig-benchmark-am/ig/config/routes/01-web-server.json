{
  "name" : "01-web-server",
  "baseURI" : "http://lee-web:80",
  "condition" : "${matches(request.uri.path, '^/static')}",
  "handler": {
    "type": "Chain",
    "config" : {
      "filters": [{
        "name": "ssoFilter",
        "type": "SingleSignOnFilter",
        "config": {
          "amService": "AmService-1"
        }
      }],
      "handler": {
        "type": "ReverseProxyHandler",
        "config": {
          "asyncBehavior": "non_streaming",
          "connections": 100,
          "numberOfWorkers": 20,
          "soTimeout": "30 seconds"
        },
        "comment": "Log request, pass it to the sample app, log response",
        "_baseURI" : "http://lee-web:80"       
      },
      "status": 200,
      "entity": "web01 page."
    }
  }
}
