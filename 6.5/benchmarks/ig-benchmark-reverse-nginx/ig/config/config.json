{
  "handler": {
    "type": "Router",
    "name": "_router",
    "baseURI": "http://ig-benchmark-web:80",
    "capture": "all"
  },
  "heap": [
    {
      "name": "JwtSession",
      "type": "JwtSession",
      "config": {
          "keystore": {
              "type": "KeyStore",
              "config": {
                  "url": "file:///var/run/secrets/openig/keystore.jks",
                  "password": "changeit"
              }
          },
          "alias": "jwe-key",
          "password": "changeit",
          "cookieName": "ig-jwt",
          "cookieDomain": ".test.com"
      }
    },
    {
      "name": "capture",
      "type": "CaptureDecorator",
      "config": {
        "captureEntity": true,
        "_captureContext": true
      }
    }
  ]
}