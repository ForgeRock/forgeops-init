{
  "handler": {
    "type": "Router",
    "name": "_router",
    "baseURI": "http://ig-benchmark-web:80",
    "capture": "all",
    "config": {
      "filters": [
        {
          "name": "basic-auth",
          "type": "HttpBasicAuthFilter",
          "config": {
            "username": "testuser",
            "password": "password",
            "failureHandler": "AuthFailureHandler",
            "cacheHeader": false
          }
        }
      ],
      "handler": {
        "name": "AuthFailureHandler",
        "type": "StaticResponseHandler",
        "config": {
          "status": 401,
          "reason": "Auth failure",
          "entity": "<html><h2>Authentication Failure</h2></html>",
          "headers": {
            "content-type": [
              "text/html"
            ]
          }
        }
      },
      "session": "JwtSession"
    }
  }
}  